using System;
using System.Linq;

public class MainClass
{
    public static void Main()
    {
        int[] array = Console.ReadLine().Split().Select(int.Parse).ToArray();
        int min = array.Min();
        int max = array.Max();
        Console.WriteLine($"Наименьшее значение = {min}");
        Console.WriteLine($"Наибольшее значение = {max}");                  
    }
}
