public static void DeltaDecoding(int[] array)
        {

            for (int i = 0; i < array.Length; i++)
            {
                if(i==0)
                    Console.Write(array[i] + " ");
                else
                {
                array[i] = array[i] + array[i-1];
                Console.Write(array[i] + " ");
                }
            }
           
                
        }

