public static Matrix Trans(Matrix A)
    {
        Matrix B = new Matrix();
        B.CountRow = A.CountColumn;
        B.CountColumn = A.CountRow;
        B.Data = new double[B.CountRow, B.CountColumn];
        for (int i = 0; i < A.CountRow; i++)
            for (int j = 0; j < A.CountColumn; j++)
                B.Data[j, i] = A.Data[i, j];
        return B;
    }
